{
  "name": "chaldene",
  "version": "0.2.4",
  "description": "Bootstrap 3 Based Admin Toolkit",
  "private": true,
  "author": "onokumus",
  "devDependencies": {
    "@types/jquery": "^3.5.14",
    "assemble": "^0.24.3",
    "concurrently": "^7.4.0",
    "gulp-extname": "^0.2.2",
    "gulp-prettify": "^0.5.0",
    "handlebars-helpers": "^0.10.0",
    "less": "^4.1.3",
    "material-shadows": "^3.0.1",
    "nodemon": "^2.0.20",
    "suitcss-components-grid": "^4.0.0",
    "suitcss-utils-display": "^1.0.2",
    "suitcss-utils-flex": "^2.0.1",
    "suitcss-utils-position": "^1.0.1",
    "suitcss-utils-size": "^2.0.1",
    "typescript": "^4.8.4"
  },
  "scripts": {
    "content": "assemble ltr",
    "content:rtl": "assemble rtl",
    "dev": "assemble",
    "build": "gulp",
    "build:rtl": "gulp rtl",
    "css": "lessc --source-map --include-path=node_modules source/less/chaldene.less docs/assets/css/chaldene.css",
    "js": "tsc -p tsconfig.json",
    "watch-js": "tsc -p tsconfig.json --watch",
    "watch-css": "nodemon -e less -x \"npm run css\"",
    "watch": "concurrently npm:watch-*"
  },
  "assemble": {
    "yarn": true
  }
}

{
  "name": "chaldene",
  "version": "0.2.5",
  "description": "Bootstrap 3 Based Admin Toolkit",
  "private": true,
  "author": "onokumus",
  "dependencies": {
    "bootstrap": "^5.3.0-alpha1"
  },
  "devDependencies": {
    "@types/jquery": "^3.5.16",
    "astro": "^2.0.4",
    "concurrently": "^7.6.0",
    "nodemon": "^2.0.20",
    "sass": "^1.57.1",
    "typescript": "^4.9.5"
  },
  "scripts": {
    "dev": "astro dev",
    "build": "astro build",
    "css:less": "lessc --source-map --include-path=node_modules source/less/chaldene.less public/assets/css/chaldene.css",
    "css:sass": "sass node_modules/bootstrap/scss/bootstrap-utilities.scss public/assets/css/bootstrap-utilities.css",
    "js": "tsc -p tsconfig.chl.json",
    "watch-js": "tsc -p tsconfig.json --watch",
    "watch-css": "nodemon -e less -x \"npm run css:less\"",
    "watch": "concurrently npm:watch-*"
  }
}

<!doctype html>
<html lang="en">

<head>
  <meta charset="utf-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <!-- The above 3 meta tags *must* come first in the head; any other head content must come *after* these tags -->
  <title>Chaldene - {{title}}</title>

  <!-- Bootstrap 5 -->
  <link rel="stylesheet" href="assets/vendor/bootstrap/css/bootstrap{{#if rtl}}.rtl{{/if}}.css">

  <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/font-awesome/css/font-awesome.min.css">
  <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/metismenu/dist/metisMenu.min.css">
  <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/animate.css">
  <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/metis-canvas/dist/metis-canvas.min.css">
  <!-- END: Vendor stylesheet files -->

  {{#if pagecss}}
  <!-- Plugin stylesheet files. OPTIONAL -->
  {{#each pagecss}}
  {{css .}}
  {{/each}}
  <!-- END: plugin stylesheet files -->
  {{/if}}

  {{#is title "Calendar"}}
  <!-- Plugin stylesheet files. OPTIONAL -->
  <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/fullcalendar@3.10.1/dist/fullcalendar.print.min.css"
    media="print">
  <!-- END: plugin stylesheet files -->
  {{/is}}


  <!-- Theme main stlesheet files. REQUIRED -->
  <link rel="stylesheet" href="assets/css/chaldene{{#if rtl}}-rtl{{/if}}{{#if prod}}.min{{/if}}.css">
  <link id="theme-list" rel="stylesheet"
    href="assets/css/theme-peter-river{{#if rtl}}-rtl{{/if}}{{#if prod}}.min{{/if}}.css">
  <!-- END: theme main stylesheet files -->

  {{#unless demo}}
  <!-- theme-switcher stylesheet files. NOT REQUIRED -->
  <link rel="stylesheet" href="assets/css/theme-switcher{{#if rtl}}-rtl{{/if}}{{#if prod}}.min{{/if}}.css">
  <!-- END: theme-switcher stylesheet file. -->

  <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/highlight.js/11.2.0/styles/github.min.css">
  {{/unless}}

</head>

<body class="app" {{#if rtl}}dir="rtl" {{/if}}>

  <!-- begin .app-wrap -->
  <div class="app-wrap">
    {% body %}
  </div>
  <!-- END: .app-wrap -->


  <span class="fa fa-angle-up" id="totop" data-plugin="totop"></span>

  {{#unless demo}}
  {{> theme-switcher}}
  {{/unless}}

  <!-- Vendor javascript files. REQUIRED -->
  <script src="https://cdn.jsdelivr.net/npm/jquery"></script>
  <script src="https://cdn.jsdelivr.net/npm/bootstrap@3.4.1/dist/js/bootstrap.min.js"></script>
  <script src="https://cdn.jsdelivr.net/npm/metismenu"></script>
  <script src="https://cdn.jsdelivr.net/npm/metis-canvas"></script>
  <script src="https://cdn.jsdelivr.net/npm/screenfull@5.2.0/dist/screenfull.min.js"></script>
  <script src="https://cdn.jsdelivr.net/npm/in-view"></script>

  <script src="https://cdn.jsdelivr.net/npm/moment"></script>
  <script src="https://cdn.jsdelivr.net/npm/moment/min/locales.min.js"></script>
  <!-- END: Vendor javascript files -->

  {{#if vendorjs}}
  <!-- Plugin javascript files. OPTIONAL -->
  {{#each vendorjs}}
  {{js .}}
  {{/each}}
  <!-- END: plugin javascript files -->
  {{/if}}


  <script src="assets/js/chaldene{{#if prod}}.min{{/if}}.js"></script>

  {{#if pagejs}}
  <!-- Demo javascript files. NOT REQUIRED -->
  {{#each pagejs}}
  {{js .}}
  {{/each}}
  <!-- END: demo javascript files -->
  {{/if}}


  {{#unless demo}}
  <!-- theme-switcher scripts file. NOT REQUIRED -->
  <script src="assets/js/theme-switcher{{#if prod}}.min{{/if}}.js"></script>
  <!-- END: theme-switcher scripts file. -->

  <script src="https://cdnjs.cloudflare.com/ajax/libs/highlight.js/11.2.0/highlight.min.js"></script>
  <script>
    $(document).ready(function () {
      hljs.highlightAll();
    });
  </script>
  {{/unless}}
</body>

</html>

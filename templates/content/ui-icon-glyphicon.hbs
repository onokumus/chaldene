---
title: Glyphicons
category: ui
subcategory: icons
vendorjs:
- https://cdn.jsdelivr.net/npm/clipboard
- https://cdn.jsdelivr.net/npm/noty@2.4.1/js/noty/packaged/jquery.noty.packaged.min.js
pagejs:
- assets/js/pagejs/ui-icon-glyphicon.js
---

<div class="row">
  <div class="col-xs-12">
    <div class="d-flex flex-row flex-wrap">
    {{#each glyphicons}}
        <div class="u-sizeFillAlt p-a-15 m-a-5 text-center bg-white b-a b-silver inview">
          <button class="btn btn-default" type="button" data-clipboard-text="glyphicon {{.}}">
              <i class="glyphicon {{.}}"></i>
          </button>
          <p>
            {{.}}
          </p>
        </div>
    {{/each}}
    </div>
  </div>
</div>

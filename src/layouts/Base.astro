---
import MetaVendor from "../components/MetaVendor.astro";
import CssVendor from "../components/CssVendor.astro";
import JsVendor from '../components/JsVendor.astro';
export interface Props {
  title: string;
}

const { title } = Astro.props;
---

<html lang="en">
  <head>
    <MetaVendor />
    <title>Chaldene - {title}</title>
    <CssVendor />

    <slot name="head" />
  </head>
  <body>
    <!-- BEGIN .app -->
    <div class="app">
      <!-- BEGIN .app-wrap -->
      <div class="app-wrap">
        <slot name="body" />
      </div>
      <!-- END .app-wrap -->
    </div>
    <!-- END .app -->
    <JsVendor />
    <slot name="footer" />
  </body>
</html>

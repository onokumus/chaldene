---
import Dashboard from "../components/Dashboard.astro";
import LayoutFirst from "../layouts/LayoutFirst.astro";
---

<LayoutFirst title="Dashboard">
  <link slot="styles" rel="stylesheet" href="https://cdn.jsdelivr.net/npm/dragula@3.7.3/dist/dragula.min.css">
  <link slot="styles" rel="stylesheet" href="https://cdn.jsdelivr.net/npm/jqvmap@1.5.1/dist/jqvmap.min.css">
  <link slot="styles" rel="stylesheet" href="https://cdn.jsdelivr.net/npm/chart.js@2.9.4/dist/Chart.min.css">
  <link slot="styles" rel="stylesheet" href="https://cdn.jsdelivr.net/npm/tablesorter@2.31.3/dist/css/theme.bootstrap_4.min.css">
  <script slot="scripts" is:inline src="https://cdn.jsdelivr.net/npm/waypoints@4.0.1/lib/jquery.waypoints.min.js"></script>
  <script slot="scripts" is:inline src="https://cdn.jsdelivr.net/npm/jquery.counterup@2.1.0/jquery.counterup.min.js"></script>
  <script slot="scripts" is:inline src="https://cdn.jsdelivr.net/npm/jqvmap@1.5.1/dist/jquery.vmap.min.js"></script>
  <script slot="scripts" is:inline src="https://cdn.jsdelivr.net/npm/jqvmap@1.5.1/examples/js/jquery.vmap.sampledata.js"></script>
  <script slot="scripts" is:inline src="https://cdn.jsdelivr.net/npm/jqvmap@1.5.1/dist/maps/jquery.vmap.usa.js"></script>
  <script slot="scripts" is:inline src="https://cdn.jsdelivr.net/npm/tablesorter@2.31.3/dist/js/jquery.tablesorter.combined.min.js"></script>
  <script slot="scripts" is:inline src="https://cdn.jsdelivr.net/npm/chart.js@2.9.4/dist/Chart.min.js"></script>
  <script slot="scripts" is:inline src="https://cdn.jsdelivr.net/npm/dragula@3.7.3/dist/dragula.min.js"></script>
  <script slot="scripts" is:inline src="/chaldene/assets/js/pagejs/dashboard.js"></script>

  <Dashboard />
</LayoutFirst>
